package handler;

import client.*;
import java.awt.event.*;

public class BtQuickRoomHandler implements ActionListener {

	ClientUI ui;
	
    public BtQuickRoomHandler(ClientUI c) {
        ui = c;
    }
    
    @Override
    public void actionPerformed(ActionEvent e) {
        ui.net.sendQuickRoomRequest();
    }
}
