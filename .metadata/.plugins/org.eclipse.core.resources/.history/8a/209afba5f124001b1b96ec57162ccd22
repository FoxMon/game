package common;

import java.io.Serializable;

public class Message implements Serializable {
	
	private boolean send;
	private boolean read;
	
    private String name;
    private String ment;
    private String date;
    
    public Message(boolean send, String name, String ment, boolean read, String date) {
    	this.send = send;
    	this.read = read;
    	this.ment = ment;
    	this.name = name;
    	this.date = date;
    }
    
}
